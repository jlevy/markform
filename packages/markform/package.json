{
  "name": "markform",
  "version": "0.1.0",
  "description": "Agent-friendly, human-readable, editable forms stored as .form.md files",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./cli": {
      "import": {
        "types": "./dist/cli.d.mts",
        "default": "./dist/cli.mjs"
      },
      "require": {
        "types": "./dist/cli.d.cts",
        "default": "./dist/cli.cjs"
      }
    },
    "./ai-sdk": {
      "import": {
        "types": "./dist/ai-sdk.d.mts",
        "default": "./dist/ai-sdk.mjs"
      },
      "require": {
        "types": "./dist/ai-sdk.d.cts",
        "default": "./dist/ai-sdk.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "markform": "./dist/bin.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:golden": "vitest run tests/golden",
    "publint": "publint"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.0",
    "@markdoc/markdoc": "^0.5.0",
    "atomically": "^2.0.0",
    "commander": "^13.0.0",
    "jiti": "^2.4.0",
    "picocolors": "^1.1.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "publint": "^0.3.0",
    "tsdown": "^0.16.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  }
}
