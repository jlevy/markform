{
  "name": "markform",
  "version": "0.1.1",
  "description": "Agent-friendly, human-readable, editable forms stored as .form.md files",
  "license": "AGPL-3.0-or-later",
  "author": "Joshua Levy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jlevy/markform.git",
    "directory": "packages/markform"
  },
  "homepage": "https://github.com/jlevy/markform#readme",
  "bugs": {
    "url": "https://github.com/jlevy/markform/issues"
  },
  "keywords": [
    "forms",
    "markdown",
    "ai",
    "agents",
    "llm",
    "structured-data",
    "markdoc",
    "ai-sdk"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./cli": {
      "types": "./dist/cli.d.mts",
      "default": "./dist/cli.mjs"
    },
    "./ai-sdk": {
      "types": "./dist/ai-sdk.d.mts",
      "default": "./dist/ai-sdk.mjs"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "markform": "./dist/bin.mjs"
  },
  "files": [
    "dist",
    "examples",
    "README.md",
    "SPEC.md"
  ],
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "copy-docs": "cp ../../README.md . && cp ../../SPEC.md .",
    "build": "pnpm run copy-docs && tsdown",
    "dev": "tsdown --watch",
    "prepare": "pnpm run copy-docs && tsdown",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:golden": "vitest run tests/golden",
    "test:golden:regen": "npx tsx scripts/regen-golden-sessions.ts",
    "test:coverage": "vitest run --coverage",
    "test:coverage:html": "vitest run --coverage && open coverage/index.html",
    "publint": "publint"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^2.0.1",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@ai-sdk/xai": "^3.0.1",
    "@clack/prompts": "^0.9.0",
    "@markdoc/markdoc": "^0.5.0",
    "atomically": "^2.0.0",
    "commander": "^13.0.0",
    "dotenv": "^17.2.3",
    "jiti": "^2.4.0",
    "js-sha256": "^0.11.1",
    "picocolors": "^1.1.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.0"
  },
  "peerDependencies": {
    "ai": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@types/node": "^24.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^6.0.3",
    "publint": "^0.3.0",
    "tsdown": "^0.16.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.0"
  }
}
